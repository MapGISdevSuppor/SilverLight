<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:control="clr-namespace:ZDIMSDemo.Controls"
    xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows"
    xmlns:NavigationPanel="clr-namespace:ZDIMSDemo.Controls">


    <Style TargetType="control:Magnifier">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="control:Magnifier">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid x:Name="LayoutRoot">
                            <Canvas Canvas.ZIndex="99" 
                                Width="300" 
                                Height="300" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top" >
                                <Ellipse x:Name="PART_Circle" 
                                     VerticalAlignment="Top" 
                                     HorizontalAlignment="Left" 
                                     Width="300" 
                                     Height="300" 
                                     Fill="#FF313131" 
                                     Opacity="0.7"
                                     StrokeThickness="12"
                                     />
                                <Canvas x:Name="imgContainer" Width="300"  Height="300" Background="White">
                                    <Canvas.Clip>
                                        <EllipseGeometry RadiusX="144" RadiusY="144" Center="150,150"></EllipseGeometry>
                                    </Canvas.Clip>
                                </Canvas>
                                <Rectangle 
                                       Stroke="#FF101010" 
                                       Height="24" 
                                       Margin="144,288,0,0" 
                                       Width="24" 
                                       Canvas.Left="0" 
                                       Canvas.Top="0"
                                       x:Name="PART_Rect" Opacity="0.8" 
                                        HorizontalAlignment="Left" 
                                        VerticalAlignment="Bottom" 
                                        RenderTransformOrigin="0.5,0.5" RadiusX="2" RadiusY="2" StrokeThickness="1" Fill="#FF181414"
                                       />
                            </Canvas>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <Style TargetType="NavigationPanel:NavigationPanel">
