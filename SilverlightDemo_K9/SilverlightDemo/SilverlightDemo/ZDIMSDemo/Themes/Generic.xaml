        <Setter Property="FontSize" Value="14"/>
        <Setter Property="HorizontalToolBarLeft" Value="85.1"/>
        <Setter Property="VerticalToolBarTop" Value="87.448"/>
        <Setter Property="NavCircleScale" Value="1.0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="NavigationPanel:NavigationPanel">
                    <Canvas x:Name="LayoutRoot" Opacity="0.82" Cursor="Hand">
                        <Canvas.Resources>
